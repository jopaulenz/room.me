<h1>Flatmates Interested in Your Property</h1>

<% @suggested_flatmates.each do |suggestion| %>
  <div class="flatmate-profile">
    <h2><%= suggestion[:flatmate].first_name %>
    <p><strong>Matching Score:</strong></p>

    <%= render partial: 'flatmate_info', locals: { flatmate: suggestion[:flatmate]} %>
  </div>
<% end %>
</div>
